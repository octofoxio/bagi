{
  "displayName": "2c2p-integration",
  "testURL": "http://localhost/",
  "verbose": true,
  "globals": {
    "ts-jest": {
      "skipBabel": true
    }
  },
  "transform": {
    "^.+\\.tsx?$": "ts-jest"
  },
  "reporters": ["default"],
  "testEnvironment": "node",
  "collectCoverageFrom": ["<rootDir>/**/*.ts"],
  "testMatch": ["<rootDir>/**/*.test.ts"],
  "testPathIgnorePatterns": ["/node_modules/"],
  "moduleFileExtensions": ["js", "jsx", "ts", "tsx", "json"]
}
